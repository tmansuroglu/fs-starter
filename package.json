{
  "name": "portfolio-v2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "npx dotenv -e .env.dev -- prisma migrate dev --name init",
    "generate": "npx dotenv -e .env.dev -- prisma generate",
    "dev": "npx dotenv -e .env.dev -- ts-node-dev --respawn --transpile-only --poll -r tsconfig-paths/register --watch src src/index.ts",
    "dev:docker": "docker compose --env-file .env.dev -f docker-compose.dev.yaml up --watch",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint --ext .ts,.tsx src",
    "format": "prettier --write \"**/*.{ts,tsx,ejs,json}\"",
    "prepare": "husky install",
    "docker:reset": "docker-compose --env-file .env.dev -f docker-compose.dev.yaml down -v  && docker image prune -af && docker volume prune -f"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.6.0",
    "bcryptjs": "^3.0.2",
    "cookie-parser": "^1.4.7",
    "csurf": "^1.10.0",
    "dotenv": "^16.5.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "rate-limiter-flexible": "^7.0.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.24.0",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.14.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "3.5.3",
    "prisma": "^6.6.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1"
  }
}
